
# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                          controllers.HomeController.querySelectionHandler()

GET     /query1                    controllers.HomeController.query1()

GET     /query2                    controllers.HomeController.query2()

GET     /query3                    controllers.HomeController.query3()

GET     /query4                    controllers.HomeController.query4()

GET     /query5                    controllers.HomeController.query5()

GET     /query21                   controllers.HomeController.query21()

GET     /query22                   controllers.HomeController.query22()

GET     /query23                   controllers.HomeController.query23()

GET     /querySelection            controllers.HomeController.querySelectionHandler()

GET     /query1Response                  controllers.HomeController.queryOneHandler()

GET     /query2Response                  controllers.HomeController.queryTwoHandler()

GET     /query3Response                  controllers.HomeController.queryThreeHandler()

GET     /query4Response                  controllers.HomeController.queryFourHandler()

GET     /query5Response                  controllers.HomeController.queryFiveHandler()

GET     /query21Response                  controllers.HomeController.queryTwoOneHandler()

GET     /query22Response                  controllers.HomeController.queryTwoTwoHandler()

GET     /query23Response              controllers.HomeController.queryTwoThreeHandler()




# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file           controllers.Assets.at(path="/public", file)
